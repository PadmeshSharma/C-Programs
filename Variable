#include<stdio.h>
int main()
{
	int a = 4;
	// int b = 4.5 ; not recommended
	float b = 5.5 ;
	char c = 'u';
	int d = 45 ;
	int e = 45 + 4 ;
	printf("The value of a is %d \n", a);
	printf("The value of b is %f \n", b);
	printf("The value of c is %c \n", c);
	printf("The sum of a and d is %d \n", a+d);
	printf("The sum of a and d is %d \n",e);
		return 0;
}
