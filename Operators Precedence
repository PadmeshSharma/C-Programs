#include<stdio.h>
int main()
{
	int x=2, y=3;
	printf("The value of 3x-8y is %d \n",3*x-8*y); 
	printf("The value of 8y/3x is %d \n",8*y/3*x);
	/* 8*3/3*2
	24/6=3 is wrong answer */
	// We will calculate acc to precedence.If it is same then associativity i.e. left to right
	/*8*3/3*2
	24/3*2
	8*2=16 */
	return 0;
 }
