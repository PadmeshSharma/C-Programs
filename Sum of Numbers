// FOR SUM OF INPUT NO.S

#include<stdio.h>
int main()
{
	int a, b, sum;
	printf("Enter two no.s");
	scanf("%d%d",&a,&b);
	sum=a+b;
	printf("Sum of these two no.s is %d",sum);
	return 0;
}

\\FOR SUM OF PARTICULAR NO.S
#include<stdio.h>
int main()
{
	int a=2;
  int b= 3;
	printf("Sum of these two no.s is %d",a+b);
	return 0;
}
